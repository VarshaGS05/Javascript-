// Type converstion:
let a=25;
a=String(a);
console.log(a,typeof a);
node /tmp/aVGTko7bhK.js
25 string
let a=25;
a=a.toString();
console.log(a,typeof a);
node /tmp/aVGTko7bhK.js
25 string

// Type converstion:String to number
var b ="222"
console.log(b,typeof b);

var a = "222";
a=Number(a);
console.log(a,typeof a)
node /tmp/aVGTko7bhK.js
222 string
222 number

// Type converstion:String to number
var a = false;
a=Number(a);
console.log(a,typeof a)
var a = true;
a=Number(a);
console.log(a,typeof a)
node /tmp/aVGTko7bhK.js
0 number
1 number

//array & names are not convert into number
var a = [1,2,3,4,5];
a=Number(a);
console.log(a,typeof a)
node /tmp/aVGTko7bhK.js
NaN number

var a = "joe";
a=Number(a);
console.log(a,typeof a)
