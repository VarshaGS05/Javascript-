//Arithmetic operators
let a = 10;
let b = 20;
let c;
c=a+b;
console.log(c);
node /tmp/aVGTko7bhK.js
30

//Arithmetic operators
let a = 10;
let b = 20;
let c;
c=a-b;
c=a*b;
c=a/b;
c=5**5
c=++a;
c=--b;
console.log(c);

//Assignment operators
let a = 10;
let b = 20;
let c;
c+=5;
c-=10;
c*=8;
c/=9;
c%=3;
console.log(c);

//Math function
let c;
c=Math.PI;
console.log(c);
